<h1>会員情報編集</h1><br>
<%= form_with model: @customer do |f| %>
<p>氏名</p><%= f.text_field :last_name %><%= f.text_field :first_name %>
<p>フリガナ</p><br><%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %>
<p>郵便番号</p><br><%= f.text_field :postal_code %>
<p>住所</p><br><%= f.text_field :address %>
<p>電話番号</p><br><%= f.text_field :telephone_number %>
<p>メールアドレス</p><br><%= f.text_field :email %>

<%= f.submit"編集内容を保存する" %>

<%= link_to "退会する",customers_confirm_path,method: :get %>
<% end %>