<h1>会員一覧</h1>

<table>
  <thead>
    <tr>
      会員ID
      氏名
      メールアドレス
    </tr>
  </thead>
  <tbody>
      <% @customer.each do |customer| %>
      <tr>
      <td><%= customer.id %></td>
      <td><%= link_to customer.last_name,admin_customer_path(customer.id)%><%= link_to customer.first_name,admin_customer_path(customer.id) %></td>
      <td><%= customer.email %></td>
      </tr>
      <% end %>
  </tbody>
</table>
